<script language="javascript">
	function createLink(id, string, accessString, linkHref) {
	
		//a11y - replace quotes with <q> ... </q>
		var QUOTESTRING = "&#034;";
		if (string.indexOf(QUOTESTRING) > -1) {
			var stringlist = string.split(QUOTESTRING);
			if (stringlist.length % 2 == 1) {
				for (var ii = 0; ii < stringlist.length; ii++) {
					if (ii % 2 == 1) {
						stringlist[ii] = "<q>" + stringlist[ii] + "</q>";
					}
				}
				string = stringlist.join("");
			}
		}
		
		var docElement = document.getElementById(id);
		if(docElement) {
			if(linkHref) {
				var linkText = string.replace('{0}', '<a href="javascript:;">');
				linkText = linkText.replace('{1}', '</a>');
				linkText = linkText.replace("'", "\'");
				docElement.innerHTML = linkText;
				
				var linkTitle = accessString.replace('{0}', '');
				linkTitle = linkTitle.replace('{1}', '');
				
				var link = docElement.getElementsByTagName('a');
				if (link.length > 0) {
					link = link[0];
					link.setAttribute("aria-label", linkTitle);
					link.onclick = function() {openHelpWindow('', linkHref); return false;};
				}
				
			} else {
				docElement.innerHTML = string;
			}
		}
	}
	
