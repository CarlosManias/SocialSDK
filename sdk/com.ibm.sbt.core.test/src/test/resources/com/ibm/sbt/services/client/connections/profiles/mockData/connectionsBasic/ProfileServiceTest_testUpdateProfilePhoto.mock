<script type="text/javascript">
submitSearch = function( ) {
	//this.searchBar is an lconn.core.SearchBar object
    var str = searchBar.getValue();
    str = dojo.string.trim(str); // trim it
    if (!str) return false;

    var selectedOption = searchBar.selectedOption;
    
    // Perform the global search if one of the features is selected
    if(selectedOption.feature) {
        return true;
    }
    else 
    {
    	var encodedSearchStr = encodeURIComponent(str); //SPR#JHUG83E588        
    	var lang = "en_us";
		switch( selectedOption.scope ){
 			case "extkeyword":
				var searchUrl = selectedOption.action + encodedSearchStr;
				window.location.replace( searchUrl );
				break;
 			case "keyword":
				var searchUrl = "https://dev.sdkdemo.com:444/profiles/html/keywordSearch.do"+"?keyword="+encodedSearchStr+((lang != "")?"&lang="+lang:""); 
				window.location.replace( searchUrl );
				break;
			case "simple":
				var searchUrl = "https://dev.sdkdemo.com:444/profiles/html/simpleSearch.do"+"?searchBy=name"+"&searchFor="+encodedSearchStr+((lang != "")?"&lang="+lang:""); 
				window.location.replace( searchUrl );
				break;
			case "advanced":
				window.location.replace('/profiles/html/advancedSearchView.do');
				break;
			default:
			   	return true;
       			break;
	    }
	}   
    
    return false; //Don't submit the form
};

searchChoiceSelected = function(item, focusTextbox ) {
	// Hack until common search widget supports onclick
	searchBar.selectOptionOrig(item, focusTextbox ); // call the original onselect function 
	if ( searchBar && searchBar.selectedOption && searchBar.selectedOption.scope && searchBar.selectedOption.scope == "advanced") { 
		window.location.replace('/profiles/html/advancedSearchView.do');
	}
	return true;
}

var searchBar = null;
if( typeof(dojo) != "undefined") {  
	dojo.addOnLoad( function(){ 
		if( typeof(lconn) != "undefined" && lconn.core && lconn.core.SearchBar ) {
		            
			searchBar = new lconn.core.SearchBar(
				{
					localOptions: [
						{label: 'Profiles by Name', 		iconClass: 'baseIcon lconnSprite lconnSprite-iconProfiles16', scope: 'simple',   action: '/profiles/html/simpleSearch.do', defaultOption: true , 'class': 'lotusAlignLeft'},
						{label: 'Profiles by Keyword', 		iconClass: 'baseIcon lconnSprite lconnSprite-iconProfiles16', scope: 'keyword',  action: '/profiles/html/keywordSearch.do' , 'class': 'lotusAlignLeft'},
						{label: 'Directory Search',	iconClass: 'baseIcon lconnSprite lconnSprite-iconProfiles16', scope: 'advanced', action: '/profiles/html/advancedSearchView.do' , 'class': 'lotusAlignLeft'}
					],
					thirdPartySearchEngines: lconn_core_thirdPartySearchEngines,
					globalOptions: [{}],
					searchContextPath: 'https://dev.sdkdemo.com:444/search',
					lblSearch:         'Search',
					lblSelectScope:    'Select search scope',
					lblAllConnections: 'All Connections',
					lblAdvanced:       'Advanced',
					
					onSubmit: submitSearch,
					'class': 'lotusAlignLeft'
				},
				dojo.byId('commonSearchControlDiv')
			);
			
			// Hack until common search widget supports onclick
			searchBar.selectOptionOrig = searchBar.selectOption;
			searchBar.selectOption = searchChoiceSelected;
		}
	});
}


</script>


