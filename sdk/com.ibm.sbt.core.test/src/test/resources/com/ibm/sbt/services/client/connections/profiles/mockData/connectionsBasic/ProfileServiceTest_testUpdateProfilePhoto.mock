<script language="javascript">
	function createLink(id, string, accessString, linkHref) {
	
		//a11y - replace quotes with <q> ... </q>
		var QUOTESTRING = "&#034;";
		if (string.indexOf(QUOTESTRING) > -1) {
			var stringlist = string.split(QUOTESTRING);
			if (stringlist.length % 2 == 1) {
				for (var ii = 0; ii < stringlist.length; ii++) {
					if (ii % 2 == 1) {
						stringlist[ii] = "<q>" + stringlist[ii] + "</q>";
					}
				}
				string = stringlist.join("");
			}
		}
		while (accessString.indexOf(QUOTESTRING) > -1) {
			accessString = accessString.replace(QUOTESTRING, '"');
		}
		
		var docElement = document.getElementById(id);
		if(docElement) {
			if(linkHref) {
				var linkText = string.replace('{0}', '<a href="javascript:;">');
				linkText = linkText.replace('{1}', '</a>');
				linkText = linkText.replace("'", "\'");
				docElement.innerHTML = linkText;
				
				var linkTitle = accessString.replace('{0}', '');
				linkTitle = linkTitle.replace('{1}', '');
				
				var link = docElement.getElementsByTagName('a');
				if (link.length > 0) {
					link = link[0];
					link.setAttribute("aria-label", linkTitle);
					link.onclick = function() {openHelpWindow('', linkHref); return false;};
				}
				
			} else {
				docElement.innerHTML = string;
			}
		}
	}
	
	createLink('profilesWelcomeMsg1', 'Perform a quick search by entering a name in the field provided.', 'Perform a quick search by entering a name in the field provided.', '');
	createLink('profilesWelcomeMsg2', 'Click &#034;Display full search options&#034; to perform a more advanced search. {0}Learn more{1}.', 'Click &#034;Display full search options&#034; to perform a more advanced search. {0}Learn more{1}.', '/topic/com.ibm.lotus.connections.profiles.help/t_pers_search_directory.html');
	createLink('profilesWelcomeMsg3', 'Learn how to {0}use tags{1} to find people.', 'Learn how to {0}use tags{1} to find people.', '/topic/com.ibm.lotus.connections.profiles.help/c_pers_tags.html');
